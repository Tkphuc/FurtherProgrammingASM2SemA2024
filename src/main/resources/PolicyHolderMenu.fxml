<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TitledPane text="Policy Holder Menu" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PolicyHolderMenuController">
   <content>
      <Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <children>
            <VBox fx:id="dependentFunctions" layoutX="97.0" layoutY="121.0" prefHeight="86.0" prefWidth="157.0">
               <children>
                  <Button fx:id="fileDependent" mnemonicParsing="false" text="File dependents' info">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="getDependent" mnemonicParsing="false" text="Get dependents' info">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="updateDependent" mnemonicParsing="false" text="Update dependents' info" />
               </children>
            </VBox>
            <VBox fx:id="policyHolderFunctions" layoutX="353.0" layoutY="121.0" prefHeight="98.0" prefWidth="127.0">
               <children>
                  <Button fx:id="getSelfInfo" mnemonicParsing="false" text="Get self's info" onAction="#switchToGetSelfInfo">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="updateSelfInfo" mnemonicParsing="false" text="Update self's info" onAction="#switchToUpdateSelfInfo"/>
               </children>
            </VBox>
            <Button fx:id="exit" layoutX="274.0" layoutY="263.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="39.0" text="Exit" onAction="#switchToLoginScreen" />
         </children>
      </Pane>
   </content>
</TitledPane>
