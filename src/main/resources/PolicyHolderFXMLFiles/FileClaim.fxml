<TitledPane maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0"
            text="File new claim"
            xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.policy_holder_controllers.FileClaimController"
>
    <Pane prefHeight="200.0" prefWidth="200.0">
        <Button fx:id="returnButton" layoutX="168.0" layoutY="331.0" mnemonicParsing="false" text="Return"
