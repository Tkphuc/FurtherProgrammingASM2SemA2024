        <TextField fx:id="examDateField" layoutX="245.0" layoutY="137.0"/>
        <TextField fx:id="claimDateField" layoutX="245.0" layoutY="183.0"/>
        <Label layoutX="90.0" layoutY="111.0" text="Claim Amount"/>
        <Label layoutX="99.0" layoutY="149.0" text="Exam Date"/>
        <Label layoutX="99.0" layoutY="187.0" text="Claim Date"/>
        <Label layoutX="105.0" layoutY="239.0" text="Documents"/>
        <Label layoutX="91.0" layoutY="54.0" text="Beneficiary ID"/>
        <TextField fx:id="customerIDField" layoutX="245.0" layoutY="50.0"/>
        <TextField fx:id="documentField" layoutX="245.0" layoutY="235.0" promptText="document"/>
    </Pane>
</TitledPane>
