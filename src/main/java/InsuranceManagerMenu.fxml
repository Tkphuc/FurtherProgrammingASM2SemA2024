<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<TitledPane maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0"
            text="Insurance Manager Menu"
            xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1"
fx:controller="InsuranceManagerMenu">
   <content>
      <Pane prefHeight="200.0" prefWidth="200.0">
         <children>
            <VBox fx:id="functions" layoutX="64.0" layoutY="67.0"
                  prefHeight="127.0" prefWidth="189.0">
               <children>
                  <Button fx:id="approveMenu" mnemonicParsing="false"
                          text="Approve/Reject proposed claims">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="retrieveAllClaims" mnemonicParsing="false"
                          text="Retrieve all claims">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="retrieveAllCustomers"
                          mnemonicParsing="false" text="Retrieve all custoners">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="retrieveSurveyors" mnemonicParsing="false"
                          text="Retrieve surveyors info">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="exit" mnemonicParsing="false" text="Exit" />
               </children>
            </VBox>
         </children>
      </Pane>
   </content>
</TitledPane>
