      xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
fx:controller="PolicyHolderMenuController">
   <children>
      <VBox fx:id="dependentFunctions" layoutX="78.0" layoutY="121.0" prefHeight="86.0" prefWidth="157.0">
