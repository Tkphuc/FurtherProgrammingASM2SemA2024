      </VBox>
      <VBox fx:id="policyHolderFunctions" layoutX="326.0" layoutY="121.0" prefHeight="98.0" prefWidth="127.0">
         <children>
            <Button fx:id="getSelfInfo" mnemonicParsing="false" text="Get self's info">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="updateSelfInfo" mnemonicParsing="false" text="Update self's info" />
